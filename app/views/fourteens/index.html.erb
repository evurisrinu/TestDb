<h1>Listing fourteens</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @fourteens.each do |fourteen| %>
      <tr>
        <td><%= fourteen.name %></td>
        <td><%= link_to 'Show', fourteen %></td>
        <td><%= link_to 'Edit', edit_fourteen_path(fourteen) %></td>
        <td><%= link_to 'Destroy', fourteen, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fourteen', new_fourteen_path %>
