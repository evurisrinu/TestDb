<h1>Listing twenties</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @twenties.each do |twenty| %>
      <tr>
        <td><%= twenty.name %></td>
        <td><%= link_to 'Show', twenty %></td>
        <td><%= link_to 'Edit', edit_twenty_path(twenty) %></td>
        <td><%= link_to 'Destroy', twenty, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twenty', new_twenty_path %>
