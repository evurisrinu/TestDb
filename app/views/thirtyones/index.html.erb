<h1>Listing thirtyones</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @thirtyones.each do |thirtyone| %>
      <tr>
        <td><%= thirtyone.name %></td>
        <td><%= link_to 'Show', thirtyone %></td>
        <td><%= link_to 'Edit', edit_thirtyone_path(thirtyone) %></td>
