<h1>Listing twentynines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @twentynines.each do |twentynine| %>
      <tr>
        <td><%= twentynine.name %></td>
        <td><%= link_to 'Show', twentynine %></td>
        <td><%= link_to 'Edit', edit_twentynine_path(twentynine) %></td>
        <td><%= link_to 'Destroy', twentynine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twentynine', new_twentynine_path %>
