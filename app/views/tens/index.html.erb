<h1>Listing tens</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tens.each do |ten| %>
      <tr>
        <td><%= ten.name %></td>
        <td><%= link_to 'Show', ten %></td>
        <td><%= link_to 'Edit', edit_ten_path(ten) %></td>
        <td><%= link_to 'Destroy', ten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ten', new_ten_path %>
