<h1>Listing twentythrees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @twentythrees.each do |twentythree| %>
      <tr>
        <td><%= twentythree.name %></td>
        <td><%= link_to 'Show', twentythree %></td>
        <td><%= link_to 'Edit', edit_twentythree_path(twentythree) %></td>
        <td><%= link_to 'Destroy', twentythree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

